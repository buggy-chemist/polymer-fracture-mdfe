safe_time		300
next_job_info		next_job.info
#
lammps_input_path	../../source/MD_code/lib/md_dpd_main_frac.in
lammps_prm_path	input_parameters/lammps.prm
lammps_exe		/home/hpc/b136dc/b136dc11/Fritz/usrapps/lammps_nhr_fritz_20220504/lammps-4May2022/src/lmp_mpi
lammps_proc             144
lammps_output_dir	MD_data
lammps_AF_dir		MD_data/
lammps_AC_dir		MD_data/
lammps_initial_data	input_files/MD.data
lammps_data_name	PS_SSP_equil
#			
matlab_exe		/usr/local/bin/matlab
matlab_code_path	../../source/FE_code
matlab_prm_path	input_parameters/matlab.prm
matlab_af_dir		FE_data/
matlab_ac_dir		FE_data/
